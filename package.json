{
   "name": "webpack",
   "version": "1.0.0",
   "description": "Este es un curo de webpack",
   "main": "index.js",
   "scripts": {
      "build": "webpack",
      "build:external": "webpack --config ./external/webpack.config.js --mode production",
      "build:multi": "webpack --config ./multi-entry-points/webpack.config.js --mode production",
      "build:css": "webpack --config ./css-style-loader/webpack.config.js --mode development",
      "build:plugins": "webpack --config ./plugins/webpack.config.js --mode development",
      "build:dev": "webpack-dev-server --config ./webpack-dev-server/webpack.config.js --mode development"
   },
   "keywords": [
      "webpack",
      "curso"
   ],
   "author": "@zamora_darwin",
   "license": "MIT",
   "dependencies": {
      "html-webpack-plugin": "3.2.0",
      "mini-css-extract-plugin": "0.8.0"
   },
   "devDependencies": {
      "css-loader": "3.2.0",
      "style-loader": "1.0.0",
      "webpack": "4.41.2",
      "webpack-cli": "3.3.9",
      "webpack-dev-server": "3.9.0"
   }
}